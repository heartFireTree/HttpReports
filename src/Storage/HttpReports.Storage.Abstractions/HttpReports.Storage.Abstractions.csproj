<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>HttpReports.Storage.Abstractions</AssemblyName>
    <PackageId>HttpReports.Storage.Abstractions</PackageId> 
    <Authors>SpringLee</Authors>
    <Company>SpringLee</Company>
    <Description>HttpReports is a lightweight APM system developed for .NET Core , it is also suitable for use in a micro-service architecture.</Description>
    <PackageProjectUrl>https://github.com/dotnetcore/HttpReports</PackageProjectUrl> 
    <RepositoryUrl>https://github.com/dotnetcore/HttpReports</RepositoryUrl>
    <PackageIcon>logo3.png</PackageIcon>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="FreeSql" Version="1.8.1" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
    <PackageReference Include="Snowflake.Core" Version="2.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\HttpReports.Core\HttpReports.Core.csproj" />
  </ItemGroup>



  <ItemGroup>
    <None Include="..\..\HttpReports.Dashboard.UI\static\logo3.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>

